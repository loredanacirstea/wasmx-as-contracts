{
    "name": "wasmx-monorepo",
    "private": true,
    "workspaces": [
      "packages/*"
    ],
    "overrides": {
      "json-as": "github:loredanacirstea/as-json#external-custom-alias2"
    },
    "scripts": {
      "install-all": "npm install",
      "reinstall-all": "rm -rf node_modules packages/*/node_modules package-lock.json && npm install"
    }
}
