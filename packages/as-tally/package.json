{
  "name": "as-tally",
  "version": "0.0.1",
  "description": "BigInt for AssemblyScript",
  "main": "index.js",
  "scripts": {
    "test": "node tests",
    "asbuild:debug": "asc assembly/index.ts --target debug",
    "asbuild:release": "asc assembly/index.ts --target release",
    "asbuild": "npm run asbuild:debug && npm run asbuild:release",
    "start": "npx serve ."
  },
  "keywords": [
    "u128",
    "u256",
    "bignum",
    "bigint",
    "biginteger",
    "fixed",
    "wasm",
    "webassembly",
    "assemblyscript"
  ],
  "author": "Loredana Cirstea",
  "license": "UNLICENSED",
  "devDependencies": {
    "assemblyscript": "0.27.36"
  },
  "dependencies": {
    "json-as": "github:loredanacirstea/as-json#nested-custom-serializer-fix"
  },
  "type": "module",
  "exports": {
    ".": {
      "import": "./build/release.js",
      "types": "./build/release.d.ts"
    }
  }
}
